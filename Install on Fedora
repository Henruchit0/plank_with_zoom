If previously installed from official Repositories, please Uninstall first then
clone https://github.com/Henruchit0/plank_with_zoom.git
cd plank
 sudo dnf install automake autoconf (skip if installed)
 sudo dnf install gettext (skip if installed)
 sudo dnf install gettext-devel (skip if installed)
 sudo dnf install libtool (skip if installed)
 sudo dnf install vala (skip if installed)
 sudo dnf install libgnome-menu* (skip if installed)
 sudo dnf install gnome-menus-devel (skip if installed)
 sudo dnf install gtk3-devel gdk-pixbuf2-devel cairo-devel libwnck3-devel (skip if installed)
 sudo dnf install libgee-devel* (skip if installed)
 sudo dnf install bamf-devel* (skip if installed)
 ./autogen.sh
 make
 sudo make install
sudo dnf install plank-libs
sudo dnf install bamf-daemon
